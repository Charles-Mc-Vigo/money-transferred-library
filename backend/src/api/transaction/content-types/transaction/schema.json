{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Amount": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    }
  }
}
